<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" prefHeight="350.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Button fx:id="btnToMain" mnemonicParsing="false" onAction="#toMainPressed" prefHeight="26.0" prefWidth="251.0" text="Analyse Data that Has Been Added">
         <VBox.margin>
            <Insets bottom="25.0" top="25.0" />
         </VBox.margin>
      </Button>
      <Label text="Current Sources of Data in Database" underline="true" />
      <ListView fx:id="lblRecordSources" prefHeight="125.0" prefWidth="350.0" VBox.vgrow="ALWAYS">
         <VBox.margin>
            <Insets bottom="5.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </ListView>
      <Button fx:id="btnDelete" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#deletePressed" prefHeight="26.0" prefWidth="250.0" text="Delete Data from Selected Source">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <VBox.margin>
            <Insets bottom="5.0" top="5.0" />
         </VBox.margin>
      </Button>
      <Button mnemonicParsing="false" onAction="#addCSVPressed" prefHeight="26.0" prefWidth="250.0" text="Add Data From csv File">
         <VBox.margin>
            <Insets bottom="10.0" top="5.0" />
         </VBox.margin>
      </Button>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</VBox>
